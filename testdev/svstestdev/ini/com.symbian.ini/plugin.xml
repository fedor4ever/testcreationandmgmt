<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.symbian.com/ini" 
       class = "com.symbian.ini.IniPackage"
       genModel = "model/ini.genmodel" /> 
  </extension>

  <extension point="org.eclipse.emf.ecore.extension_parser">
    <parser 
       type="ini" 
       class="com.symbian.ini.util.IniResourceFactoryImpl" />
  </extension>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.symbian.com/comment" 
       class = "com.symbian.comment.CommentPackage"
       genModel = "model/ini.genmodel" /> 
  </extension>

</plugin>

